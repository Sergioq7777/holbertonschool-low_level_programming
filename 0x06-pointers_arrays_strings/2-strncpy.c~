#include "holberton.h"

/**
 * *_strncpy - check the code for Holberton School students.
 *@dest: char
 *@src: char
 *@n : integer
 * Return: Void.
 */

char *_strncpy(char *dest, char *src, int n)
{
	int  b;

	for (b = 0 ; src[b] != '\0' && b < n  ; b++)
	{
		dest[b] = src[b];
	       
	}
	return (dest);
}
