#include "holberton.h"
#include <stdio.h>
#include <stdlib.h>

/**
 * *string_nconcat -  concatenates two strings.
 * @n: int
 * @s2: char
 * @s1 char
 * Return: Never.
 */

char *string_nconcat(char *s1, char *s2, unsigned int n)

{
	int i, j;

	for (i = 0 ; s1[i] != '\0' ; i++)
		;
	for (j = 0 ; s2[j] != '\0' ; i++)
		;

}
