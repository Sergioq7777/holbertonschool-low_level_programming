#include "holberton.h"

/**
 * *_strcat - check the code for Holberton School students.
 *@dest: char
 *@src: char
 * Return: Void.
 */

char *_strcat(char *dest, char *src)
{
	int s1, s2;

	for (s1 = 0; dest[s1] != '\0' ; s1++)
		;
	for (s2 = 0; src[s2] != '\0' ; s2++, s1++)
	{
		dest[s1] = src[s2];
	}

	dest[s1] = '\0';
	return (0);

}
